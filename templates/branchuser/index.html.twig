{% extends 'base.html.twig' %}

{% block title %}Branchuser index{% endblock %}

{% block body %}
    <h1>Branchuser index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for branchuser in branchusers %}
            <tr>
                <td>{{ branchuser.id }}</td>
                <td>
                    <a href="{{ path('branchuser_show', {'id': branchuser.id}) }}">show</a>
                    <a href="{{ path('branchuser_edit', {'id': branchuser.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('branchuser_new') }}">Create new</a>
{% endblock %}
